if ! hascondition trigger slots
	chat trigger " You have to be playing slots to do this."
	endscript
end
if ! hascondition trigger playing
	chat trigger " Your not playing slots."
	endscript
end
if ! variableisstring param0
	chat trigger " How many coins do you wish to enter??"
endscript
end
number money param0
if ! variableisnumber money
	chat trigger " Parameter is not a number. "
endscript
end
if > money 1000
	chat trigger " The maximum bet is 1000. "
endscript
end
if < money 5
	chat trigger " The minimum bet is 5. "
endscript
end
if < trigger cash money
	chat trigger " You havent got that much money. "
endscript
end
number reel1 * 1 * 6 rand
number reel2 * 1 * 6 rand
number reel3 * 1 * 6 rand
number reel4 * 1 * 6 rand
number reel5 * 1 * 6 rand
chat trigger concat concat concat concat concat concat concat concat concat  concat concat " - " reel1 " - " reel2 " - " reel3 " - " reel4 " - " reel5 " - "
if = reel1 0
	if = reel2 0
		if = reel3 0
			if = reel4 0
				if = reel5 0
					number winnings * 10 * 5 + param0 rand
					inc trigger cash winnings
					chat trigger concat concat " 5 0's in a row, you win " winnings " gp. "
				endscript
				end
				number winnings * 10 * 3 + param0 rand
				inc trigger cash winnings
				chat trigger concat concat " 4 0's in a row, you win " winnings " gp. "
			endscript
			end
			number winnings * 5 * 2 + param0 rand
			inc trigger cash winnings
			chat trigger concat concat " 3 0's in a row, you win " winnings " gp. "
		endscript
		end
		number winnings * 1 * 1 + param0 rand
		inc trigger cash winnings
		chat trigger concat concat " 2 0's in a row, you win " winnings " gp. "
	endscript
	end
end
