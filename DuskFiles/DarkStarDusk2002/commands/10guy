if < trigger privs 4 endscript end
if variableisstring param0	
	livingthing target param0
	if ! isplayer target
		chat trigger "They are not in this world."
		endscript
	end
	set target maxHp 100
	set target hp target maxHp
	set target maxMp 100
	set target mp target maxMp	
	set target strength 10
	set target intelligence 10
	set target dexterity 10
	set target wisdom 10
	set target constitution 10
	set target strength 10
end
endscript


