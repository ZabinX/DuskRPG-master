#
# exited the room to the east
#
if hascondition trigger maze4
    removecondition trigger maze4
end
if hascondition trigger maze3
    removecondition trigger maze3
end
if hascondition trigger maze2
    removecondition trigger maze2
end
if hascondition trigger maze1
    removecondition trigger maze1
    addcondition trigger maze2
end
set trigger locxy 125 199
endscript
